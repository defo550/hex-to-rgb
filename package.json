{
  "name": "@defo550/hex-to-rgb",
  "version": "0.0.0-semantically-released",
  "description": "Convert hex color to rgb value.",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel src -d dist --ignore *.spec.js",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js -p",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "commit": "git-cz",
    "cover": "nyc npm t",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "prebuild": "rimraf dist",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "mocha src/index.spec.js --compilers js:babel-register",
    "test:watch": "npm t -- -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/defo550/hex-to-rgb.git"
  },
  "keywords": [
    "hex",
    "rgb"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Christopher DeFelice <defo550@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/defo550/hex-to-rgb/issues"
  },
  "homepage": "https://github.com/defo550/hex-to-rgb#readme",
  "engines": {
    "node": ">= 6.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.22.0",
    "babel-register": "6.22.0",
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.9.5",
    "cz-conventional-changelog": "1.2.0",
    "ghooks": "2.0.0",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "npm-run-all": "4.0.1",
    "nyc": "10.1.2",
    "rimraf": "2.5.4",
    "semantic-release": "6.3.2",
    "webpack": "2.2.1"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    }
  }
}
